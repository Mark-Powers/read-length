{
    "manifest_version": 2,
    "name": "Read Length",
    "version": "1.1.1",

    "description": "Calculate length of read",

    "icons": {
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["read-length.js"]
        }
    ],

    "options_ui": {
        "page": "options.html"
    },

    "permissions": [
        "storage"
    ]
}

